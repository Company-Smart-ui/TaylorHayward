mixin sAlbums()
	// start sAlbums
	+b.SECTION.sAlbums.section#sAlbums&attributes(attributes)
		.container
			+b.section-title.text-center
				h2 Music
			+e.widget
				<iframe src="https://open.spotify.com/embed/artist/4U5MAmJil1tym2GO72tSh7?utm_source=generator" width="100%" height="380" frameBorder="0" allowfullscreen="" allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture"></iframe>

	// end sAlbums