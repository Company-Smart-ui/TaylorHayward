mixin sEvents()
	// start sEvents
	+b.SECTION.sEvents.section#sEvents&attributes(attributes)
		+b.flyItems.fly-items-js
			+e.item--1(data-depth="0.1")
				+icon('music-solid')
			+e.item--2(data-depth="0.2")
				+icon('music-solid')
			+e.item--3(data-depth="0.3")
				+icon('piano')
			+e.item--4(data-depth="0.1")
				+icon('piano')
			+e.item--5(data-depth="0.4")
				+icon('note-piano')
			+e.item--6(data-depth="0.1")
				+icon('note-piano')
			+e.item--7(data-depth="0.5")
				+icon('note-piano')
			+e.item--8(data-depth="0.5")
				+icon('music-solid')
			+e.item--9(data-depth="0.1")
				+icon('play')
			+e.item--10(data-depth="0.5")
				+icon('audio')
			+e.item--11(data-depth="0.6")
				+icon('audio')
		.container
			.row
				.col
					+b.section-title
						h2 Music
						+e.showAll.h5(href="/music.html") Listen to everything
				.col-auto
					+e.controls
						button.eventSlider-prev(type="button")
							img(src="img/chevron-left-brown.svg", loading="lazy" alt="slider nex")
						button.eventSlider-next(type="button")
							img(src="img/chevron-right-brown.svg", loading="lazy" alt="slider prev")
			+b.eventSlider
				.swiper-container.eventSlider-js
					.swiper-wrapper
						.swiper-slide
							<iframe src="https://open.spotify.com/embed/album/7oJyCNSFoqNJt5L2A9pkBX?utm_source=generator" width="100%" height="380" frameBorder="0" allowfullscreen="" allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture" loading="lazy"></iframe>
						.swiper-slide
							<iframe src="https://open.spotify.com/embed/album/1ugD376bCCHgoOyIEHRxL9?utm_source=generator" width="100%" height="380" frameBorder="0" allowfullscreen="" allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture" loading="lazy"></iframe>
						.swiper-slide
							<iframe src="https://open.spotify.com/embed/album/4IGVS2XOD3iDzoREJMccvC?utm_source=generator" width="100%" height="380" frameBorder="0" allowfullscreen="" allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture" loading="lazy"></iframe>
						.swiper-slide
							<iframe src="https://open.spotify.com/embed/album/3uZKswAssh5KJB64RwyMwp?utm_source=generator" width="100%" height="380" frameBorder="0" allowfullscreen="" allow="autoplay; clipboard-write; encrypted-media; fullscreen; picture-in-picture" loading="lazy"></iframe>

	// end sEvents